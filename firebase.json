{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [{
      "source": "/",
      "destination": "/sites-survey.html",
      "type": 302
      },{
      "source": "/index.html",
      "destination": "/sites-survey.html",
      "type": 302
      },{
      "source": "/map.html",
      "destination": "/sites-survey.html",
      "type": 302
      },{
      "source": "/_index.html",
      "destination": "/index.html",
      "type": 302
      },{
      "source": "/_map.html",
      "destination": "/map.html",
      "type": 302
    }]
  }
}
